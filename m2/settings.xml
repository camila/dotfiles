<?xml version="1.0" encoding="UTF-8"?>
<settings>
    <mirrors>
        <mirror>
          <id>artifactory</id>
          <name>artifactory</name>
          <url>http://artifactory.intranet/repo</url>
          <mirrorOf>*</mirrorOf>
        </mirror>
    </mirrors>
    <profiles>
        <profile>
              <id>notests</id>
              <activation>
                <property>
                  <name>!skipTests</name>
              </property>
            </activation>
            <properties>
             <skipTests>true</skipTests>
            </properties>
        </profile>
    </profiles>
</settings>
